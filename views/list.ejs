<%post.forEach(function(element){%>
<div class="box1" >
   <h1 class="box-h1"><%=element.title%></h1>

   <p><%=element.bodycontent.substring(0,100)+" ..."%>  
      <a href="/posts/<%=element.title%>">Read more</a> </p>
  
</div>
<%})%>
</div>
