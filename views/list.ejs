<%-include('header'); -%>

<div id="fixed"><a href="/compose"><textarea name="" id="" cols="20" rows="3" placeholder="compose your blog here"></textarea></a></div>

<div class="main-div">

<div class="box1" >
   <h1 class="box-h1">Home</h1>
   <p><%=content%></p>
</div>

